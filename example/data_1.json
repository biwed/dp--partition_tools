{
  "kind": "partitioning",
  "api": "v2",
  "schema": "partitioning_tools_test",
  "tags": [
    "partitioning",
    "v2",
    "datamanagement"
  ],
  "email": [
    "Aleksandr.Tkachev@leroymerlin.ru",
    "Andrei.Larionov1@leroymerlin.ru"
  ],
  "start_date": "2021-04-01",
  "owner": "dm",
  "access_control": [
    "datamanagement",
    "agg_ruptures"
  ],
  "schedule": "0 0 1 * *",
  "tables": [
    {
      "table": "item_loc_soh",
      "operations": [
        {
          "operation": "create_partitions",
          "granularity": "1 year",
          "lower_bound": "2 year",
          "upper_bound": "10 month"
        },
        {
          "operation": "create_partitions",
          "granularity": "3 month",
          "lower_bound": "1 year",
          "upper_bound": "1 month"
        },
        {
          "operation": "create_partitions",
          "granularity": "1 month",
          "lower_bound": "3 month",
          "upper_bound": "-3 month"
        },
        {
          "operation": "merge_partitions",
          "granularity": "3 month",
          "lower_bound": "1 year",
          "upper_bound": "3 month",
          "table_space": "warm",
          "limit_operations": 2,
          "access_exclusive_mode": false
        },
        {
          "operation": "move_partitions",
          "lower_bound": "5 year",
          "upper_bound": "1 year",
          "table_space": "warm",
          "limit_operations": 2,
          "access_exclusive_mode": false
        }
      ]
    }
  ]
}